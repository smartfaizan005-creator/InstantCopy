<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <exclude domain="database" path="app_settings.db"/>
        <exclude domain="sharedpref" path="app_settings.xml"/>
        <exclude domain="cache" path="cache/"/>
    </cloud-backup>
    <device-transfer>
        <exclude domain="database" path="app_settings.db"/>
        <exclude domain="sharedpref" path="app_settings.xml"/>
    </device-transfer>
</data-extraction-rules>